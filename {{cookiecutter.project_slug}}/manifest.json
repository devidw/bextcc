{
    "name": "{{ cookiecutter.project_name }}",
    "description": "{{ cookiecutter.project_short_description }}",
    "version": "{{ cookiecutter.version }}",
    "manifest_version": 3,
    "permissions": [],
    "background": {},
    "content_scripts": [
        {
            "matches": [
                "*://example.org/*"
            ],
            "js": [
                "./src/js/vendor/jquery-3.6.0.min.js",
                "./src/js/inject.js"
            ],
            "css": [
                "./src/css/inject.min.css"
            ]
        }
    ]
}